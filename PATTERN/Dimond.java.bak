class  Dimond
{
	public static void main(String[] args) 
	{
		int n=6;
		for(int i=0;i<n;i++)
		{
			for(int j=0;j<n;j++)
			{
				if(i>n/2)
					break;
				if((i+j)>=n/2 && (i+j)/2 != 0)
					System.out.print("*  ");
                else
                   System.out.print("  ");
				
			}
System.out.println();
}
			
	}
}
