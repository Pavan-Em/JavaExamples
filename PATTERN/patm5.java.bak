class patm5
{
	public static void main(String[]args)
	{
		int n=5;
		int x=0;
		for (int i=0;i<n ;i++ )
		{
			int y=0;
			for (int j=0;j<n ;j++ )
			{
				if(i>=j)
				{
					if(x==4)
					{
						System.out.print(x +" ");
						x=0;
					}
					else
						System.out.print(x++ +" ");
					y++;	
				}
			}
			x=y;
			System.out.println();
		}
	}
}