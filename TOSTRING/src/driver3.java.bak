class name
{
	private String name;
	private date birthday;

	name(String name,date birthday)
	{
		this.name=name;
		this.birthday=birthday;
	}

	public String toString()
	{
		return "My NAme is "+name+", my Birthday is on "+birthday;
	}
}
class date
{
	private int day;
	private int mon;
	private int year;

	date(int day,int mon,int year)
	{
		this.day=day;
		this.mon=mon;
		this.year=year;

		System.out.println(this);
	}

	public String toString()
	{
		return day+"/"+mon+"/"+year;
	}
}

