class p5
{
	public static void main(String[]args)
	{
	int n=5;
	int x=1;int y=0;
	for (int i=0;i<n ;i++ )
	{
		{
		for (int j=0;j<n ;j++ )
		{
			if(i>=j)
			{
				if (i%2==0)
			{
				System.out.print(x++ +" ");
				y=x+1;
			}
				else
				{
					System.out.print(y--+" ");
					x++;
				}
			}
			
			else
				System.out.print(" ");
		}
		}
		
		System.out.println();
	}
	}
}